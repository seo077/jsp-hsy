	<script type="text/javascript">
		function allCheck() {
			const all = document.querySelector('#all').checked;
			if (all === false) {
				let check ="";
				<%
				for (int i = 1; i < 5; i++) {%>
					check = document.querySelector('#check<%=i %>');
					check.removeAttribute('checked');
					
				<%}
				%>
			} else {
				<%
				for (int i = 1; i < 5; i++) {%>
					check = document.querySelector('#check<%=i %>');
					check.setAttribute('checked', 'true');
				<%}
				%>
				document.location.href = "#btn";

			}
		};

		function reload() {
			document.location.href = "#title";
			window.location.reload();
		};

		function checkVal(form) {
			const alert = document.querySelector("#alert");
			
			let check ="";
			<%for (int i = 1; i < 5; i++) {%>
				check = document.querySelector('#check<%=i %>');
				if (check.required) {
					if (!check.checked) {
						alert.innerText = "사이트 이용약관과 개인정보 수집 및 이용에 대한 안내 모두 동의해주세요.";
						return;
					}
				}
			<%}%>

			//다음 페이지로 이동
			alert.innerText = "";
			form.submit();
			return;
		};
	</script>
